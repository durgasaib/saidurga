#Namespace

apiVersion: v1
kind: Namespace
metadata:
  name: dev

-----

apiVersion: apps/v1
kind: Deployment
metadata:
  name: durgasaib
spec:
  replicas: 1
  selector:
    matchLabels:
      app: durgasaib
  template:
    metadata:
      labels:
        app: durgasaib
    spec:
      containers:
      - name: c1
        image: durgasaib/durgasaib
        ports:
        - containerPort: 80
        
----------

apiVersion: v1
kind: Service
metadata:
  name: ecomm-svc
  namespace:qa
spec:
  type: NodePort
  selector:
    app: ecomm
  ports:
    app: ecomm
  ports:
    - ports:80
      targetPort:80
      nodeport: 30008







